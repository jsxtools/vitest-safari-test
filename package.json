{
	"name": "@jsxtools/vitest-safari-test",
	"version": "0.0.0",
	"description": "Testing Vitest integration with Safari browser using WebDriverIO",
	"author": "Jonathan Neal <jonathantneal@hotmail.com> (https://jonneal.dev)",
	"homepage": "https://github.com/jsxtools/vitest-safari-test",
	"license": "MIT-0",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"repository": "https://github.com/jsxtools/vitest-safari-test",
	"bugs": "https://github.com/jsxtools/vitest-safari-test/issues",
	"sideEffects": false,
	"keywords": ["vitest", "safari", "webdriverio", "browser-testing"],
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist coverage *.tsbuildinfo",
		"format": "dprint fmt",
		"format:check": "dprint check",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"prepublishOnly": "npm run clean && npm run build && npm test",
		"test": "vitest run",
		"test:refocus": "./scripts/test-safari.sh",
		"test:watch": "vitest",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/node": "24.10.9",
		"@vitest/browser-playwright": "4.0.18",
		"@vitest/browser-webdriverio": "4.0.18",
		"@vitest/coverage-istanbul": "4.0.18",
		"@vitest/ui": "4.0.18",
		"dprint": "0.51.1",
		"playwright": "1.58.1",
		"typescript": "5.9.3",
		"vitest": "4.0.18",
		"webdriverio": "9.24.0"
	}
}
