{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": false,
		"incremental": true,
		"lib": ["ES2024"],
		"module": "es2022",
		"moduleResolution": "bundler",
		"outDir": "dist",
		"preserveConstEnums": false,
		"rootDir": "src",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2024",
		"types": []
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
